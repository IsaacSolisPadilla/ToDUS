{
  "expo": {
    "name": "ToDUS",
    "slug": "ToDUS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icono.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#084F52"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.isasolpad.frontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icono.png",
        "backgroundColor": "#084F52"
      },
      "package": "com.isasolpad.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "22a828c9-2e6a-4eb5-88e2-0547e6370d4c"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
